{
    "builds": [
      {
        "src": "api/server.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "10gb",
            "runtime": "python3.10"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/server.py"
      }
    ]
}